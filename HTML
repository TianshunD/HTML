<!--列表标签-->
<ol type="I"> <!--有序列表-->
	<li>cass</li> <!--列表-->
	<li>dean</li>
	<li>sam</li>
</ol>

<ul> <!--无序列表，没有序列-->
	<li>cass</li> <!--列表-->
	<li>dean</li>
	<li>sam</li>
</ul>

